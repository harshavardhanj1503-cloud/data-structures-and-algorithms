public class SelectionSort :  SortingAlgorithm{

	public int[] sort(int[] arr){
	
		for(int i=0;i<arr.Length-1;i++){
			int index=i;
			for(int j=i;j<arr.Length-1;j++){
			   if(arr[index]>arr[j]){
			      index=j;
			   }
			}
			int temp=arr[index];
			arr[index]=arr[i];
			arr[i]=temp;
		 
		}
		return arr;
	}
